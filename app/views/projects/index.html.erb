<h1> Projects <h1>
  <% @projects.each do |project| %>
    <h2><%= project.title %><h2>
    <h4><%= project.subtitle %><h4>
    <p><%= project.body %><p>
    <p><%= project.main_image %><p>
    <% unless project.thumb_image.nil? %>
      <%= image_tag project.thumb_image %>
    <% end %>
  <% end %>
